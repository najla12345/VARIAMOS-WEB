<shapes>
<!-- begin adaptation states model -->
<shape name="activity" aspect="variable">
	<background>
		<path>
			<move x="0" y="0"/> 
			<line x="100" y="0"/>  
			<line x="100" y="100"/>  
			<line x="0" y="100"/> 
			<line x="0" y="0"/>   
			<close/>
			<move x="10" y="0"/>
			<line x="10" y="100"/> 
			<close/>
			<move x="90" y="0"/>
			<line x="90" y="100"/> 
			<close/>
		</path>		
	</background>
	<foreground>
		<fillstroke/>
	</foreground>
</shape>

<shape name="controlAction" aspect="variable">
	<background>
		<path>
			<move x="75" y="0"/> 
			<line x="100" y="50"/>  
			<line x="75" y="100"/> 
			<line x="25" y="100"/> 
			<line x="0" y="50"/> 
			<line x="25" y="0"/>  
			<line x="75" y="0"/> 
			<close/>
		</path>		
	</background>
	<foreground>
		<fillstroke/>
	</foreground>
</shape>
 
